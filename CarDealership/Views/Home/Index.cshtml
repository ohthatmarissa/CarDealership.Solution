@using CarDealership.Models;

<h1>Welcome to the Car Dealership</h1>
<ul>
@if (Model.Count == 0)
{
  <p>You have no cars in your Dealership!</p>
}
@if(Model.Count != 0)
{
  @foreach(Car car in Model)
  {
    <li>@car.GetMakeModel(), @car.GetPrice(), @car.GetMiles(), @car.GetMessage()</li>
  }
}

</ul>
<a href="/car/new">Add a new car.</a>
